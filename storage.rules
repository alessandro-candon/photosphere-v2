rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /{allPaths=**} {
      allow read: if request.auth != null && request.auth.token.email == '<your-email>';
    }
    match /photosphere_albums.json {
      allow write: if request.auth != null && request.auth.token.email == '<your-email>';
    }
  }
}